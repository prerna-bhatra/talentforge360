generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/client"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Candidate {
  id                Int      @id @default(autoincrement())
  name              String
  email             String
  phone             String
  skills            String
  status            String?    
  qualification     String?
  expectedSalary    Int
  nodeJsExperience  Int
  reactJsExperience Int
  totalScore        Int
}
